
public class Employee {
	private int id;
	private String name;
	private int salary;
	private String Address;

	public Employee() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Employee(String name, int salary, String address) {
		super();
		
		this.name = name;
		this.salary = salary;
		Address = address;
	}
    

}
